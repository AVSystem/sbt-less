# Compile a less file

> assets
$ exists target/web/public/main/css/a.css
$ exists target/web/public/main/css/a.css.map

# Compile with compression

> set com.typesafe.sbt.less.SbtLessPlugin.LessKeys.compress in com.typesafe.sbt.web.SbtWebPlugin.WebKeys.Assets := true
> assets
-$ exists target/web/public/main/css/a.css
-$ exists target/web/public/main/css/a.css.map
$ exists target/web/public/main/css/a.min.css
$ exists target/web/public/main/css/a.min.css.map
> checkMapFileContents

